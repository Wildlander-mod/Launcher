{
  "name": "wildlander-launcher",
  "version": "2.19.0",
  "description": "A launcher for the Wabbajack modpacks",
  "author": "Wildlander",
  "scripts": {
    "clean": "lb-clean dist",
    "precompile": "npm run clean",
    "compile": "lb-tsc -b tsconfig.json",
    "postcompile": "tsc-alias",
    "prestart": "npm run compile",
    "start": "vue-cli-service electron:serve",
    "start:dev": "nodemon",
    "prebuild": "npm run compile",
    "build": "vue-cli-service electron:build",
    "lint": "npm run lint:typescript && npm run lint:eslint && npm run lint:styles && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "electron:debug:main": "electron --remote-debugging-port=9223 ./dist_electron",
    "electron:debug:renderer": "npm run start -- --debug",
    "lint:eslint": "vue-cli-service lint",
    "lint:eslint:fix": "vue-cli-service lint --fix",
    "lint:styles": "stylelint **/*.scss",
    "lint:typescript": "lb-tsc --noEmit -b tsconfig.json",
    "lint:prettier": "prettier -l \"src/**/*.{ts,tsx,js,jsx,vue,json,scss,css}\"",
    "lint:prettier:fix": "npm run lint:prettier -- --write",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test": "lb-nyc cross-env TS_NODE_PROJECT=./src/__tests__/tsconfig.json lb-mocha",
    "generate:modpack-files": "ts-node scripts/generate-modpack-files.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src/**"
    ],
    "ignore": [
      "src/renderer/**"
    ],
    "exec": "npm run start",
    "ext": "*"
  },
  "main": "background.js",
  "dependencies": {
    "@loopback/boot": "^5.0.10",
    "@loopback/context": "^5.0.10",
    "@loopback/core": "^4.0.10",
    "@vueform/toggle": "^2.1.1",
    "electron-context-menu": "^3.4.0",
    "electron-log": "^4.4.8",
    "electron-shutdown-command": "^2.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^4.6.5",
    "fetch-installed-software": "^0.0.7",
    "fs-extra": "^11.1.1",
    "js-ini": "^1.6.0",
    "junk": "^3.1.0",
    "mitt": "^3.0.0",
    "node-fetch": "^2.6.7",
    "ps-list": "^7.2.0",
    "tslib": "^2.0.0",
    "vue": "^3.2.31",
    "vue-class-component": "^8.0.0-0",
    "vue-final-modal": "^3.4.4",
    "vue-router": "^4.1.3",
    "vue3-click-away": "^1.2.4",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/config-conventional": "^15.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@loopback/build": "^9.0.10",
    "@loopback/eslint-config": "^14.0.2",
    "@loopback/testlab": "^5.0.10",
    "@tsconfig/node16-strictest": "^1.0.4",
    "@tsconfig/strictest": "^2.0.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/ini": "^1.3.31",
    "@types/mocha": "^10.0.1",
    "@types/mock-fs": "^4.13.1",
    "@types/mock-require": "^2.0.1",
    "@types/ncp": "^2.0.5",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.9",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "babel-eslint": "^10.1.0",
    "commitlint": "^15.0.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.5",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^8.42.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^9.14.1",
    "lint-staged": "^12.1.3",
    "mocha": "^10.2.0",
    "mock-fs": "^5.1.3",
    "mock-require": "^3.0.3",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.7",
    "reflect-metadata": "^0.1.13",
    "sass-loader": "^10.2.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.21.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.6.4",
    "tsconfig-paths": "^4.0.0",
    "typescript": "~4.9.5",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-property-decorator": "^10.0.0-rc.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "footer-max-line-length": [
        0,
        "always"
      ]
    },
    "parserPreset": {
      "parserOpts": {
        "noteKeywords": [
          "docs:"
        ]
      }
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V "
  },
  "repository": "https://github.com/Wildlander-mod/Launcher"
}
